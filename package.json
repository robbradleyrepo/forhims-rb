  "scripts": {
    "api:mock": "NODE_ENV=testing npm run dev",
    "dev": "concurrently npm:build:local npm:start",
    "clean": "find . -name '.DS_Store' -type f -delete",
    "scaffold": "node ./tools/scaffold",
    "build:local": "parallel-webpack --watch --progress --colors",
    "build": "NODE_ENV=production webpack",
    "build:lambda": "LAMBDA=true NODE_ENV=production webpack --progress",
    "start": "wait-on dist/server.js && nodemon dist/server.js",
    "startAptible": "NODE_ENV=production node --max_old_space_size=2048 dist/server.js",
    "eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "lint": "prettier --write '**/*.js'",
    "lint:css": "stylelint './**/*.style.js'",
    "lint:fix": "eslint --fix . --ext .js",
    "precommit": "lint-staged",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress:open": "cypress open",
    "cypress:open:mock": "CYPRESS_MOCKSERVER=true cypress open",
    "cypress:open:staging": "CYPRESS_baseUrl=http://app-11755.on-aptible.com cypress open",
    "cypress:run": "cypress run",
    "cypress:run:mock": "CYPRESS_MOCKSERVER=true cypress run"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "cypress",
      "testconfig.js",
      ".component.js",
      ".style.js",
      "index.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ]
